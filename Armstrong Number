#include <iostream>
#include <cmath>
using namespace std;

//Check Armstrong

int main() 
{
	int n,m,a=0,t;
	cin>>n;
	m=n;
	while(m>0)
	{
		t=m%10;
		a=a+pow(t,3);
		m=m/10;
	}
	if(n==a)
	{
		cout<<"It is an Armstrong Number\n";
	}
	else
	{
		cout<<"It is not an Armstrong Number\n";
	}
	return 0;
}
