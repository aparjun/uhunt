#include <iostream>
#include <string.h>

using namespace std;

//Check Palindrome

int main()
{
	int flag=0;
	string str;
	cin>>str;
	for(int i=0,j=str.length()-1;i<=j;i++,j--)
	{
	  if(str[i]!=str[j])
	  {
	  	flag=1;
	  }
	}
	if(flag==0)
	{
		cout<<"It's a Palindrome\n";
	}
	else
	{
		cout<<"It's not a Palindrome\n";
	}
	return 0;
}
