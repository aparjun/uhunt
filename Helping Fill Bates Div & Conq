#include <iostream>
#include <algorithm>
#include <string.h>
using namespace std;

int main()
{
	    string a,b;
	    cin>>a;
		int n,i=0;
		cin>>n;
		while(i<n)
		{
			cin>>b;
			int c=0,d=0,x=0,y=0,al,bl;
			al=a.length();
			bl=b.length();
			while(x<al && y<bl)
			{
				if(a[x]==b[y])
				{
					c++;
					if(c==1)
					{
						d=x;
					}
					x++;
					y++;
				}
				else
				{
				    x++;	
				}
			}
			if(c==bl)
			{
			  cout<<"Matched "<<d<<" "<<x-1<<endl;	
			}
			else
			{
			  cout<<"Not matched"<<endl;
			}
			
			i++;
		}
		
	    return 0;
}
